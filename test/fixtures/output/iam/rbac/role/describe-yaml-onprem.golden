name: DeveloperRead
accesspolicy:
    scopetype: Resource
    allowedoperations:
        - resourcetype: Cluster
          operations: []
        - resourcetype: TransactionalId
          operations:
            - Describe
        - resourcetype: Group
          operations:
            - Read
            - Describe
