{
	"aggregations": [
		{
  		"metric": "io.confluent.kafka.server/retained_bytes"
		}
	],
	"filter": {
		"op": "OR",
		"filters": [
	    {
    		"field": "metric.label.cluster_id",
    		"op": "EQ",
    		"value": "lkc-gk9dxm"
  		}
		]
	},
	"granularity": "PT15M",
	"group_by": [
		"metric.label.cluster_id"
	],
	"intervals": [
		"now-3d|d/now|d"
	],
	"limit": 1000
}
